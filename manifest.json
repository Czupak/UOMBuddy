{
  "manifest_version": 3,
  "name": "UOMBuddy",
  "version": "0.2",
  "description": "Displays prices as price/weight",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "js": [
        "scripts/content.js"
      ],
      "matches": [
        "https://botland.com.pl/*-filamenty-*",
        "https://www.123-3d.nl/*"
      ]
    }
  ]
}